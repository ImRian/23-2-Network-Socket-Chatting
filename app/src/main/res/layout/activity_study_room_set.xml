<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:textColor="#000000"
    tools:context=".StudyRoomSetActivity">


    <EditText
        android:id="@+id/roomSubTitle"
        android:layout_width="match_parent"
        android:layout_height="77dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="603dp"
        android:ems="10"
        android:inputType="text"
        android:text="스터디룸 환경관리"
        android:textColor="#000000"
        android:textStyle="bold"
        android:paddingLeft="20dp"/>

    <RelativeLayout
        android:id="@+id/roomTitleBar"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#0000FF">


        <RelativeLayout
            android:id="@+id/roomTitleBar2"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="#7993E1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="Study Room"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_alignParentBottom="true">


        <Button
            android:id="@+id/btnRoomToSG"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="StudyGroup"
            android:textStyle="bold"
            android:textSize="6pt"
            android:textColor="#000000"
            android:background="#00FFFFFF"/>


        <Button
            android:id="@+id/btnRoomToSR"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="StudyRoom"
            android:textStyle="bold"
            android:textSize="6pt"
            android:textColor="#000000"
            android:background="#00FFFFFF"/>

        <Button
            android:id="@+id/btnRoomToChat"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Chatting"
            android:textStyle="bold"
            android:textSize="6pt"
            android:textColor="#000000"
            android:background="#00FFFFFF"/>
    </LinearLayout>

    <EditText
        android:id="@+id/editTextText9"
        android:layout_width="409dp"
        android:layout_height="81dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="522dp"
        android:ems="10"
        android:inputType="text"
        android:text="빔프로젝터 전원"
        android:textColor="#000000"
        android:paddingLeft="20dp"/>

    <EditText
        android:id="@+id/editTextText10"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="450dp"
        android:ems="10"
        android:inputType="text"
        android:text="스터디룸 문 잠금"
        android:textColor="#000000"
        android:paddingLeft="20dp"/>

    <EditText
        android:id="@+id/editTextText15"
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="375dp"
        android:ems="10"
        android:inputType="text"
        android:text="조명"
        android:textColor="#000000"
        android:paddingLeft="20dp"/>

    <Switch
        android:id="@+id/switch1"
        android:layout_width="92dp"
        android:layout_height="59dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="533dp" />

    <Switch
        android:id="@+id/switch2"
        android:layout_width="92dp"
        android:layout_height="59dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="461dp" />

    <Switch
        android:id="@+id/switch3"
        android:layout_width="92dp"
        android:layout_height="59dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="388dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="63dp"
        android:layout_height="32dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="346dp"
        android:layout_marginBottom="337dp"
        android:autoSizeTextType="uniform"
        android:text="에어컨"
        android:textColor="#000000"
        android:paddingLeft="20dp" />

    <TextView
        android:id="@+id/temperatureNum"
        android:layout_width="63dp"
        android:layout_height="32dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="155dp"
        android:layout_marginBottom="250dp"
        android:autoSizeTextType="uniform"
        android:textColor="#000000"
        android:text="22" />

    <ImageButton
        android:id="@+id/upBTN"
        android:layout_width="77dp"
        android:layout_height="48dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="242dp"
        app:srcCompat="@android:drawable/arrow_up_float"
        tools:ignore="MissingConstraints, SpeakableTextPresentCheck"
        />

    <ImageButton
        android:id="@+id/downBTN"
        android:layout_width="75dp"
        android:layout_height="48dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="239dp"
        android:layout_marginBottom="241dp"
        app:srcCompat="@android:drawable/arrow_down_float"
        tools:ignore="MissingConstraints, SpeakableTextPresentCheck" />

</RelativeLayout>